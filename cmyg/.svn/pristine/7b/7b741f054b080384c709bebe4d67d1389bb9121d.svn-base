<!DOCTYPE HTML>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0, minimum-scale=1.0, user-scalable=0, initial-scale=1.0, width=device-width" />
    <meta name="format-detection" content="telephone=no, email=no, date=no, address=no">
    <title>兑换积分</title>
    <link rel="stylesheet" type="text/css" href="../../../css/mui.min.css" />
    <link rel="stylesheet" type="text/css" href="../../../css/api.css" />
    <link rel="stylesheet" type="text/css" href="../../../css/common.css" />
    <link rel="stylesheet" type="text/css" href="../../../fonts/iconfont.css" />
    <style>
    body{width: 100%;height: 100%;position:relative;}
    #page-bag .mui-scroll-wrapper {
        position: relative;
    }
    header{margin-bottom:10px;}
    .head{padding:20px;display:flex;}
    .head div{text-align: center;font-size:13px;flex:1;}
    .head .left{color:#5EA877;border-right:1px solid #ccc;}
    .head .left span i{background: url(../../../image/23464364.png) no-repeat;width: 20px;height: 20px;background-size: 20px;display:inline-block;vertical-align: top;margin-right: 5px;}
    .head .right span i{background: url(../../../image/23442I.png) no-repeat;width: 20px;height: 20px;background-size: 20px;display:inline-block;vertical-align: top;margin-right: 5px;}
    .head .right{color:#EE6F1F;}
    .bili{height: 120px;width: 120px;border-radius:50%;border:2px solid #DCDEDD;margin:0 auto;padding:15px 0;position:relative;margin-top:30px;}
    .bili span{width: 100%;height: 33%;text-align: center;}
    .bili span:nth-child(1) i{background:url(../../../image/u200.png) no-repeat;width: 15px;height: 15px;background-size: 100%;margin:0 auto;display:inline-block;}
    .bili span:nth-child(2){color:#E61C4C;font-size:25px;}
    .tab_box{padding:0 15px;margin-top:40px;display:flex;}
    .tab_box li{flex:1;margin-right:5px;text-align: center;background-color: #5F5D5E;color:#fff;border-radius: 7px;padding: 3px 0;}
    .actives{background: #E61C4C!important;}
    .box_bot li{height: 200px;background-color: #E61C4C;color:#fff;width: 90%;margin:0 auto;border-radius:10px;margin-top:15px;display:none;position:relative;}
    .bot_active{display: block!important;}
    .foot{background-color:#E61C4C;color:#fff;text-align: center;height: 50px;line-height: 50px;width: 70%;margin:0 auto;margin-top:50px;font-size:18px;border-radius: 10px;}
    .box_bot li div{}
    .box_bot li span{position:absolute;right: 5px;top:5px;color:#fff;z-index:10000;width: 100%;border-bottom:2px solid #fff;text-align: right;}
    </style>
</head>

<body>

    <div class="page" id="page-bag">
        <!-- 主页面容器 -->
        <div class="mui-inner-wrap">
            <!-- 主页面标题 -->
            <header id="header" class="mui-bar mui-bar-nav">
                <h1 class="mui-title">购物分兑换比值</h1>
                <a class=" mui-icon mui-icon-left-nav  hides"></a>
            </header>
            <div class="mui-content mui-scroll-wrapper">
                <div class="bili">
                    <span><i></i></span>
                    <span>1.12</span>
                    <span>兑换比</span>
                </div>
                <div class="box_top">
                    <ul class="tab_box">
                        <li class="actives">今天</li>
                        <li>昨天</li>
                        <li>前天</li>
                        <li>月</li>
                    </ul>
                </div>
                <div class="box_bot">
                    <ul>
                        <li class="bot_active">
                            <div class="dv1">
                            </div>
                            <span>平均值：1.22</span>
                        </li>
                        <li>
                            <div>
                            </div>
                            <span>平均值：2.22</span>
                        </li>
                        <li>
                            <div>
                            </div>
                            <span>平均值：5.72</span>
                        </li>
                        <li>
                            <div>
                            </div>
                            <span>平均值：1.92</span>
                        </li>
                    </ul>
                </div>
                <div class='foot'>我要买分</div>
            </div>
        </div>
    </div>
</body>
<script type="text/javascript" src="../../../script/api.js"></script>
<script type="text/javascript" src="../../../script/mui.min.js"></script>
<script type="text/javascript" src="../../../script/zepto.min.js"></script>
<script type="text/javascript" src="../../../script/template-native.js"></script>
<script type="text/javascript" src="../../../script/common.js"></script>
<script type="text/javascript">
    apiready = function() {
        app.ready()
        var TOP=$('.box_bot').offset().top+30
        var L=$('.box_bot ul li').offset().left+10
        var H=$('.box_bot').height()-30
        var W=$('.box_bot ul li').width()-35
        var UILineChart = api.require('UILineChart');
        var datas=[100, 10, 50, 0, 90,10,40,60,70]
        var a=UILineChart.open({
            rect: {
                x: L,
                y: TOP,
                w: W,
                h: H
            },
            xAxis: {
                indexs: ['12:00', '12:30', '13:00', '13:30', '14:00', '12:30', '13:00', '13:30', '14:00'],
                screenXcount: 8
            },
            yAxis: {
                max: 100,
                min: 0,
                step: 20,
                base: 0
            },
            datas: [
                datas
                // [-200, 100, -100, 0, -50]
            ],
            styles: {
                xAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                yAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                coordinate: {
                    bg: '#E61C4C',
                    color: '#E61C4C',
                    baseColor: '#fff',
                },
                colors: ['#fff', '#fff']
            },
            // fixedOn: api.frameName,
            fixed: false
        }, function(ret, err) {
            if (ret) {
                // alert(JSON.stringify(ret));
            } else {
                alert(JSON.stringify(err));
            }
        });
        var b=UILineChart.open({
            rect: {
              x: L,
              y: TOP,
              w: W-10,
              h: H
            },
            xAxis: {
                indexs: ['一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月'],
                screenXcount: 8
            },
            yAxis: {
                max: 100,
                min: 0,
                step: 20,
                base: 0
            },
            datas: [
                [20, 10, 50, 0, 90,10,40,60,70],
                // [-200, 100, -100, 0, -50]
            ],
            styles: {
                xAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                yAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                coordinate: {
                    bg: '#E61C4C',
                    color: '#E61C4C',
                    baseColor: '#fff',
                },
                colors: ['#fff', '#fff']
            },
            // fixedOn: api.frameName,
            fixed: false
        }, function(ret, err) {
            if (ret) {
                // alert(JSON.stringify(ret));
            } else {
                alert(JSON.stringify(err));
            }
        });
        var c=UILineChart.open({
            rect: {
              x: L,
              y: TOP,
              w: W,
              h: H
            },
            xAxis: {
                indexs: ['一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月', '十一月', '十二月', '一月'],
                screenXcount: 7
            },
            yAxis: {
                max: 100,
                min: 0,
                step: 20,
                base: 0
            },
            datas: [
                [20, 20, 50, 0, 20],
                // [-200, 100, -100, 0, -50]
            ],
            styles: {
                xAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                yAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                coordinate: {
                    bg: '#E61C4C',
                    color: '#E61C4C',
                    baseColor: '#fff',
                },
                colors: ['#fff', '#fff']
            },
            // fixedOn: api.frameName,
            fixed: false
        }, function(ret, err) {
            if (ret) {
                // alert(JSON.stringify(ret));
            } else {
                alert(JSON.stringify(err));
            }
        });
        var d=UILineChart.open({
            rect: {
              x: L,
              y: TOP,
              w: W,
              h: H
            },
            xAxis: {
                indexs: ['一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月', '十一月', '十二月', '一月'],
                screenXcount: 7
            },
            yAxis: {
                max: 100,
                min: 0,
                step: 20,
                base: 0
            },
            datas: [
                [30, 10, 10, 90, 90],
                // [-200, 100, -100, 0, -50]
            ],
            styles: {
                xAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                yAxis: {
                    bg: '#fff',
                    markColor: '#fff',
                    markSize: 12
                },
                coordinate: {
                    bg: '#E61C4C',
                    color: '#E61C4C',
                    baseColor: '#fff',
                },
                colors: ['#fff', '#fff']
            },
            // fixedOn: api.frameName,
            fixed: false
        }, function(ret, err) {
            if (ret) {
                // alert(JSON.stringify(ret));
            } else {
                alert(JSON.stringify(err));
            }
        });

     UILineChart.hide({
        id: 3
     });
     UILineChart.hide({
        id: 2
     });
     UILineChart.hide({
        id: 1
     });
     UILineChart.hide({
        id: 5
     });
    }
    $('.tab_box li').on('tap', function() {
        $(this).addClass('actives').siblings().removeClass('actives')
        var idx = $(this).index()
        $('.box_bot ul li').eq(idx).addClass('bot_active').siblings().removeClass('bot_active')
        var UILineChart = api.require('UILineChart');
        if(idx==0){
          UILineChart.hide({
             id: 3
          });
          UILineChart.hide({
             id: 2
          });
          UILineChart.hide({
             id: 1
          });
          UILineChart.show({
            id: 0
          });
        }
        if(idx==1){
          UILineChart.hide({
             id: 3
          });
          UILineChart.hide({
             id: 2
          });
          UILineChart.show({
             id: 1
          });
          UILineChart.hide({
            id: 0
          });
        }
        if(idx==2){
          UILineChart.hide({
             id: 3
          });
          UILineChart.show({
             id: 2
          });
          UILineChart.hide({
             id: 1
          });
          UILineChart.hide({
            id: 0
          });
        }
        if(idx==3){
          UILineChart.show({
             id: 3
          });
          UILineChart.hide({
             id: 2
          });
          UILineChart.hide({
             id: 1
          });
          UILineChart.hide({
            id: 0
          });
        }

    })
    $('.hides').on('tap', function() {
        api.closeWin()
    })
</script>

</html>
