        header{margin-bottom:10px;}
        .red{color: red;}
        .box{margin-top:25px;}
        .box .head{padding:0 25px;height: 50px;line-height: 50px;width: 100%;box-sizing: border-box;}
        .box .head div{width: 33%;float:left;text-align: center;color:#8B8B8B;}
        ul{padding:0 15px;width: 100%;box-sizing: border-box;;}
        ul li{height: 35px;line-height: 35px;width: 100%;font-size:14px;}
        ul li span{text-align: center;}
        ul li span:nth-child(2){border-left:2px solid #FFF;border-right:2px solid #fff;}
        ul li:nth-child(odd){background-color: #fff;color:#757575;}
        ul li:nth-child(even){background-color: #EEEEEE;color:#757575;}
        ul li:nth-child(1){background-color: #8B8B8B;color:#fff;}
        ul li:nth-child(1) span {text-align: center;}
        ul li span:nth-child(1){width: 42%;float:left;}
        ul li span:nth-child(2){width: 18%;float:left;}
        ul li span:nth-child(3){width: 40%;float:left;}
    </style>
