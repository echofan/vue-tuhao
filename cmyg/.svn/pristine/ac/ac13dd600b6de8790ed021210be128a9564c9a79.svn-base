<script type="text/javascript" src="../../../script/api.js"></script>
<script type="text/javascript" src="../../../script/mui.min.js"></script>
<script type="text/javascript" src="../../../script/zepto.min.js"></script>
<script type="text/javascript" src="../../../script/common.js"></script>
<script type="text/javascript">
  //头部的更换

    apiready = function() {
        app.ready()
    }
    // $('')
    $('.hides').on('tap',function(){
       api.closeWin()
    })
    $('.set').on('tap',function(){
       $('.hide_box').show()
    })
    $('.delete').on('tap',function(){
      $('.hide_box').hide()
    })
    $('.sets').on('tap',function(){
      api.openWin({
        name: 'buy-status',
        url: './sell_allocated.html',
        pageParam: {
          name: 'test'
        }
      });
    })
    $('.await_ok').on('tap',function(){
      api.openWin({
        name: 'buy-status',
        url: './sell_await_ok.html',
        pageParam: {
          name: 'test'
        }
      });
    })
    $('.complate').on('tap',function(){
      api.openWin({
        name: 'buy-status',
        url: './sell_complate.html',
        pageParam: {
          name: 'test'
        }
      });
    })
    $('.freeze').on('tap',function(){
      api.openWin({
        name: 'buy-status',
        url: './sell_freeze.html',
        pageParam: {
          name: 'test'
        }
      });
    })

    $('.cx').on('tap',function(){
      api.closeToWin({
        name:'root'
      })
      api.execScript({
          name: 'root',
          script: 'openFrame("bag")'
      })
      api.execScript({
          name: 'root',
          frameName: 'bag',
          script: 'reloadFrames(4)'
      })
      api.execScript({
          name: 'root',
          frameName: 'bag',
          script: 'titles("售后客服")'
      });
    })
</script>

