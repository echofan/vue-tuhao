/*登陆注册页面*/

.page-lr-main .mui-checkbox.mui-left label {
    padding-left: 42px;
}

.page-lr-main .mui-checkbox.mui-left input[type=checkbox] {
    left: 15px;
}

.page-lr-main .mui-checkbox input[type=checkbox]:before {
    font-size: 24px;
}

.content-title {
    font-size: 1.5rem;
    text-align: center;
    line-height: 1;
    margin-top: 2rem;
    color: #E50043;
}

.page-lr-main {
    width: 100%;
    height: 100%;
    overflow-x: hidden;
}

.page-lr-main {
    background: url(../image/page-bk.png) -1px -1px no-repeat;
    background-size: 101% 101%;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    justify-content: space-between;
}

.page-lr-main .mui-input-group {
    background: transparent;
    color: #999;
}

.page-lr-main .content-wrap {
    height: 100%;
    margin: 0 2rem;
}

.page-lr-main .mui-input-group::before, .page-lr-main .mui-input-group::after {
    display: none;
}

.rg-input-group {
    margin: 2rem 0;
}

.page-lr-main .mui-input-group .mui-input-row {
    margin-top: 1rem;
}

.page-lr-main .mui-input-group .mui-input-row::after {
    display: none;
}

.content-footer label {
    marign-left: 10px;
    border-bottom: 1px solid;
}

.content-footer {
    font-size: 14px;
    color: #999;
    margin: 2rem 1rem 2rem;
    line-height: 2;
}

.mui-bar.mui-bar-tab .iconfont::before {
    font-size: 20px;
}

input.input-code {
    padding-right: 4rem;
}

@keyframes mrotate {
    from {
        transform: rotate(0);
        -ms-transform: rotate(0);
        -moz-transform: rotate(0);
        -webkit-transform: rotate(0);
        -o-transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
    }
}

@-webkit-keyframes mrotate {
    from {
        transform: rotate(0);
        -ms-transform: rotate(0);
        -moz-transform: rotate(0);
        -webkit-transform: rotate(0);
        -o-transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
    }
}

@-moz-keyframes mrotate {
    from {
        transform: rotate(0);
        -ms-transform: rotate(0);
        -moz-transform: rotate(0);
        -webkit-transform: rotate(0);
        -o-transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
    }
}

@-o-keyframes mrotate {
    from {
        transform: rotate(0);
        -ms-transform: rotate(0);
        -moz-transform: rotate(0);
        -webkit-transform: rotate(0);
        -o-transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
    }
}

.span-code {
    padding: 4px;
    border-radius: 4px;
    position: absolute;
    bottom: 0;
    font-size: 14px;
    color: #efefef;
    right: 0;
    margin-bottom: 5px;
    background: #999;
    min-width: 5em;
    text-align: center;
}

.span-code.loading i.mui-icon-reload {
    vertical-align: middle;
    animation: mrotate 500ms linear infinite;
    -webkit-animation: mrotate 500ms linear infinite;
    /* Safari 和 Chrome */
}

.span-code.loading, .span-code.active {
    background: #E50043;
}

i {
    display: inline-block;
    line-height: 1.2;
    overflow: hidden;
}

i>img {
    width: 100%;
    height: 100%;
}

.fl-right {
    float: right;
}

h1.title {
    font-size: 15px;
    font-weight: normal;
    line-height: 1.2;
    word-break: break-all;
}

em {
    color: #E50043;
    font-size: 10px;
    margin-right: 4px;
}
.price-line div{float:left;}
.price-line div:nth-child(1){position:absolute;bottom:10px;left:32%;}
.price-line div:nth-child(2){position:absolute;bottom:10px;left:55%;}
.numbox {
    display: inline-flex;
    display: -webkit-inline-flex;
    /*justify-content: space-between;*/
    border: 1px solid rgba(0, 0, 0, .2);
    border-radius: 4px;
    height: 24px;
    position: absolute;
right: 5px;
bottom: 10px;
}

.numbox>* {
    width: 24px;
    display: inline-block;
    text-align: center;
    line-height: 24px;
}

.numbox input {
    padding: 0;
    margin: 0;
    height: 22px;
    padding: 0 4px;
    width: 28px;
    border-bottom: none;
    border-top: none;
    text-align: center;
    border-radius: 0;
    font-size: 12px;
}

.icon-jewelry {
    margin: 0 2px -4px;
}

i.icon-jewelry::before {
    font-size: 16px !important;
}

.data-null {
    background: url(../image/shop-empty.png) 50% 50% no-repeat;
    position: fixed;
    top: 40%;
    width: 100%;
    text-align: center;
    height: 64px;
    line-height: 140px;
    transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    font-size: 12px;
    color: #bfbfbf;
}
