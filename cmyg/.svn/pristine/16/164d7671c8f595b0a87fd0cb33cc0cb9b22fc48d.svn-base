html, body {
    height: 100%;
}
a{outline:0 none;text-decoration:none;color:black;color:black;}
/* 清除浮动 */
.clearfix:after{content:".";clear:both;display:block;height:0;line-height:0;font-size:0;visibility:hidden;}
.clearfix{zoom:1;}
/*重写mui样式*/

.mui-bar .mui-btn-link {
    color: #E50043;
}
.greens{color:green;}
.reds{color:red;}
.blues{color:#45ADDB;}
.times{color:#A0A0A0;}

/*积分交易弹框*/
.hide_box{background:rgba(0,0,0,0.6);width: 100%;height: 100%;position:fixed;top:0;left:0;display:none;}
.hide_box .box{width: 90%;height: 80%;position:absolute;top:10%;left:5%;background-color: #4A4647;color:#fff;padding:20px;}
.hide_box .box .heads span{display:block;}
.hide_box .box .heads span:nth-child(1){font-size:20px;}
.hide_box .box .heads i{background:url(../image/xx_03.jpg) no-repeat;width: 30px;height: 30px;background-size: 100%;position:absolute;right:5px;top:5px;}
.hide_box .box ul{margin-top:20px;}
.hide_box .box ul li{height: 30px;line-height: 30px;display:flex;}
.hide_box .box ul li span:nth-child(1){flex:1;}
.hide_box .box ul li span:nth-child(2){flex:4;}
.hide_box .box .mys{width: 50%;height: 40px;background-color: #E61C4C;color:#fff;text-align: center;font-size:18px;line-height: 40px;border-radius:8px;margin-top:20px;}
.hide_box .box .img_box{width: 40%;}
.hide_box .li_box{position:relative;}
.hide_box .img_box{position:absolute;width: 40%;right:0;bottom:0;top:0;}
.hide_box .img_box img{width: 100%;height: 100%;background-size: 100%;}
.hide_box .img_box .lookDetail{position:absolute;bottom:0;left:0;z-index:0;width: 100%;height: 30px;line-height: 30px;text-align: center;color:#fff;background:red;}
.hide_box .big_img{position:fixed;width: 90%;height: 90%;display:none;top:5%;left:5%;}
.hide_box .big_img img{width: 100%;height: 100%;background-size: 100%;}
.hide_box .big_img .delete{background:url(../image/654734.png) no-repeat;width: 30px;height: 30px;background-size: 100%;position:absolute;bottom:10px;left:50%;margin-left:-15px;}
.explain{position:absolute;bottom:5px;padding:0 10px 0 0;}
.explain p{color:#fff;}
.sign{}
.load-more{display:none;padding:15px;}
