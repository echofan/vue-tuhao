<script type="text/javascript">
  //头部的更换

    apiready = function() {
        app.ready()
    }
    // $('')
    $('.hides').on('tap',function(){
       api.closeWin()
    })
    $('.set').on('tap',function(){
       $('.hide_box').show()
    })
    $('.delete').on('tap',function(){
      $('.hide_box').hide()
    })
    $('.sets').on('tap',function(){
      api.openWin({
        name: 'buy-status',
        url: './buy_cancel_status.html',
        pageParam: {
          name: 'test'
        }
      });
    })
    $('.await_ok').on('tap',function(){
      api.openWin({
        name: 'buy-status',
