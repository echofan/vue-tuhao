    apiready = function(){
        var map = api.require('bMap');
        map.open({
            rect: {
                x: 0,
                y: 0,
                w: 360,
                h: 480
            },
            center: {
                lon: '',
                lat: ''
            },
            zoomLevel: 10,
            showUserLocation: true,
            fixedOn: api.frameName,
            fixed: true
            }, function(ret) {
