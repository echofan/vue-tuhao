        <style>
        .mui-inner-wrap{position:relative!important;}
        .mui-scroll-wrapper{position:relative!important;padding-bottom:100px;}
        .item_box .uls li{display:flex;height: 50px;line-height: 50px;padding:0 20px;box-sizing:border-box;border-bottom: 1px solid #ccc;}
        .item_box .uls li span:nth-child(1){flex:1;text-align: center;}
        .item_box .uls li span:nth-child(2){flex:5;padding-left:20px;}
        .uls{position:relative;}
        .one{height: 50px;border-bottom:1px solid #ccc;padding: 0 20px;line-height: 50px;}
        .one span:nth-child(1){float:left;}
        .one span:nth-child(2){float:right;}
        .uls .right{position:absolute;right:0;height: 100%;width: 35%;border-left:1px solid #ccc;top:0;background-color: #fff;border-bottom:1px solid #ccc;text-align: center;}
        .right .box{width: 90%;height: 80%;margin:0 auto;margin-top:10%;}
         .right .box img{width: 100%;height: 100%;background-size: 100%;}
         .right span{text-align: center;margin-top:3px;}
        .ggf{color:#D88F61;}
        .price{color:#656364;}
        .ggf i{background:url(../../../image/3213532.png) no-repeat;width: 20px;height: 20px;background-size: 20px;vertical-align: sub;margin-right: 3px;}
        .price i{background:url(../../../image/241I.png) no-repeat;width: 20px;height: 20px;background-size: 20px;vertical-align: sub;margin-right: 3px;}
        .fukuan{display:flex;height: 50px;line-height: 50px;padding:0 20px;}
        .fukuan span:nth-child(1){flex:1;text-align: center;}
        .fukuan span:nth-child(2){flex:5;padding-left: 20px;}
        .skf{height: 50px;padding:0 20px;line-height: 50px;background-color: #E61C4C;color:#fff;font-size:18px;}
        .ul_skf{background-color: #f0f0f2;}
        .ul_skf li{display:flex;height: 50px;line-height: 50px;padding:0 20px;box-sizing:border-box;border-bottom: 1px solid #ccc;}
        .ul_skf li span:nth-child(1){flex:1;text-align: center;}
        .ul_skf li span:nth-child(2){flex:5;padding-left:30px;}
        .residue{height: 60px;padding:0 20px;line-height: 60px;display:flex;}
        .residue span{flex:1;text-align: center;}
        .foot{}
        </style>
</head>

