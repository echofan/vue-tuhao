    <link rel="stylesheet" type="text/css" href="../../fonts/iconfont.css" />
    <style>
    .clear{clear: both;}
    .page-search-bar {display: flex; display: -webkit-flex; color: #333; padding: 10px 10px 0; align-items: stretch; background: #E60144; z-index: 10; position: fixed;width: 100%;height: 50px;}
    .page-search-bar>div:nth-child(2n+1) { align-items: center; display: flex; display: -webkit-flex; margin: 0 2px; }
    .page-search-bar span {        white-space: nowrap;    }
    .page-search-bar .i-search { position: absolute; right: 5px;bottom: 0;transform: translateY(-50%);   -webkit-transform: translateY(-50%);  }
    .page-search-bar i.iconfont.i-search::before {color: #E60144; }
    .page-search-bar .ip-search[type=text] { margin: 0;background: hsla(0, 0%, 100%, 0.85);border-radius: 4px;padding: 0;border: none;height: 30px;}
    .page-search-bar>div:nth-child(2) {flex: auto;position: relative;height: 40px;margin: 0 5px;    }
    .page-search-bar>div>span {font-size: 14px;color: #fff;line-height: 1.5;height: 25px;    }
    .page-search-bar i.iconfont::before { color: #fff; font-size: 20px;  }
    i.iconfont.icon-comments { margin-right: 4px; }

    .bar{height: 40px;width: 100%;position: fixed;background: #fff;top: 50px;z-index: 99;}
    .bar ul{width: 200%;overflow: hidden;}
    .bar ul li{float: left;width: 50px;font-style: normal !important;height: 40px;line-height: 40px;text-align: center;}
    .active{color: #E60144; }

    .lineImg{position:relative; }
    .lineImg img{width: 100%;height: 180px;float: left;}
    .lineImg .option{height: 40px;background:rgba(0,0,0,.8);position: absolute;top:140px;z-index: 1;width: 100%;}
    .info{padding: 10px;display: flex;width: 100%;overflow: hidden;}
    .left img{width: 80%;height: 80;float: left;}
    .left {display: inline-block; width: 25%; float: left;}
    .right{width: 75%;height: 70px; display: inline-block; float: left;}
    .orderInfo{overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
    /*.orderPrice{display: flex;text-align: center; vertical-align: middle;}*/
    .orderPrice div{display: inline-block;width: 32%; margin-top: 28px;}
    .price{font-size: 20px;color: #E60144; }
    .score i {background: url("../../image/312321I.png")no-repeat; width:19px;height: 19px;background-size: 16px; display: inline-block; vertical-align: middle;  }
    .score b{font-size: 8px;font-weight: 100;background:#E60144;border-radius: 50%;color: #fff;padding: 2px;position: relative;top: -10px;}
    .sale{font-size: 10px;}

    .lineCont li{border-bottom: 1px solid #ccc;margin-bottom: 10px;}
    .option span{float: left; color: #fff;}
    .name{line-height: 40px;padding-left: 10px;width: 70%;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
    .buyBtn{background: #E60144;padding:4px 30px;border-radius: 30px;margin-top: 5px;}
    </style>
</head>

