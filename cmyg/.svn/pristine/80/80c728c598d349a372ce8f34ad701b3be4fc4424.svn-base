        .boxs_item ul{border-left: 1px solid #ccc;border-top:1px solid #ccc;font-size:13px;}
        .boxs_item ul li{background-color: #fff;border-bottom: 1px solid #ccc;display:flex;height: 50px;line-height: 50px;width: 100%;text-align: center;}
        .boxs_item ul li:nth-child(1){height: 35px;line-height: 35px;background-color: #FF6600;color:#fff;}
        .boxs_item ul li div:nth-child(1){flex:3;}
        .boxs_item ul li div:nth-child(2){flex:4;}
        .boxs_item ul li div:nth-child(n+3){flex:2;}
        .boxs_item ul li div{border-right:1px solid #ccc;}
        .boxs_item ul li:nth-child(n+2) div:nth-child(1){line-height: 22px;}
        .boxs_item ul li .set{line-height: 22px;}
        .boxs_item ul li .set i{background:url(../../../image/7878UI.png) no-repeat;width: 25px;height: 25px;background-size: 25px;display:block;margin:0 auto;margin-top:3px;}
        .green{color:green;}
        .red{color:red;}
        .hide_box{position:fixed;top:0;left:0;background-color:rgba(0,0,0,0.4);width: 100%;height: 100%;z-index:1000;display:none;}
        .hide_box .boxs{position:fixed;width: 90%;height: 80%;left:50%;margin-left:-45%;top:55%;margin-top:-70%;background-color: #FF6600;border-radius:10px;}
        .hide_box .boxs .conts{height: 100%;position:relative;}
        .hide_box .boxs .top{height: 17%;}
        .hide_box .boxs .head{height: 50px;line-height: 50px;color:#fff;font-size:23px;padding:0 15px;position:relative;}
        .hide_box .boxs .head i{background:url(../../../image/shouhuo_delet.png) no-repeat;width: 25px;height: 25px;background-size: 25px;position:absolute;right:10px;top:12px;}
        .hide_box .boxs .time{height: 30px;line-height: 30px;padding:0 15px;color:#000;font-size:20px;}
        .hide_box .content{padding:0 15px;height: 57%;}
        .hide_box .content .left{width: 50%;float:left;font-size:15px;height: 100%;}
        .hide_box .content .left div{height: 20%;margin-top:5px;}
        .hide_box .content .left div span{display:block;}
        .hide_box .content .left .foot{background-color: #66CC33;width: 75%;height: 35px;line-height: 35px;text-align: center;border-radius:5px;color:#fff;font-size:16px;}
        .hide_box .content .right{width: 50%;float:right;height: 100%;}
        .hide_box .content .right img{width: 100%;height: 100%;background-size: 100%;}
        .hide_box .foot_box{padding:0 15px;margin-top:5px;height: 20%;position: absolute;bottom:0;width: 100%;box-sizing: border-box;}
        .hide_box .foot_box ul{border:1px solid #ccc;background-color: #fff;font-size:13px;}
        .hide_box .foot_box ul li{height: 35px;line-height: 35px;display: flex;}
        .hide_box .foot_box ul li:nth-child(1){border-bottom:1px solid #ccc;}
        .hide_box .foot_box ul li span{text-align: center;top:0;left:0;}
        .hide_box .foot_box ul li span{flex:1;border-right: 1px solid #ccc;}
        .hide_box .foot_box ul li span:last-child{flex:2;border-right:none;}
